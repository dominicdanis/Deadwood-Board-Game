<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="bigPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gameOverController">
   <children>
      <VBox id="dayLabel" alignment="CENTER" prefHeight="400.0" prefWidth="600.0" stylesheets="@boardStyle.css">
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="500.0" text="THE LAST DAY HAS ENDED" textAlignment="CENTER" />
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="500.0" text="THE FINAL STANDINGS ARE:" />
            <Label fx:id="finalStandings" alignment="CENTER" prefHeight="17.0" prefWidth="600.0" text="Label" wrapText="true" />
            <Button fx:id="closeButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#closeProgram" text="End Game" />
         </children>
      </VBox>
   </children>
</AnchorPane>
